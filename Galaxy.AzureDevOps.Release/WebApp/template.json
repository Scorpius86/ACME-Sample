{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appService_ACME-Sample_name": {
      "type": "string"
    },
    "appServicePlan_Galaxy.Linux_location": {
      "type": "string"
    },
    "appServicePlan_Galaxy.Linux_name": {
      "type": "string"
    },
    "appServicePlan_Galaxy.Linux_resourceGroup": {
      "type": "string"
    }
  },
  "resources": [
    {
      "location": "[parameters('appServicePlan_Galaxy.Linux_location')]",
      "name": "[parameters('appService_ACME-Sample_name')]",
      "type": "Microsoft.Web/sites",
      "apiVersion": "2015-08-01",
      "tags": {
        "[concat('hidden-related:', resourceId(parameters('appServicePlan_Galaxy.Linux_resourceGroup'),'Microsoft.Web/serverfarms', parameters('appServicePlan_Galaxy.Linux_name')))]": "empty"
      },
      "kind": "app",
      "properties": {
        "name": "[parameters('appService_ACME-Sample_name')]",
        "serverFarmId": "[resourceId(parameters('appServicePlan_Galaxy.Linux_resourceGroup'),'Microsoft.Web/serverfarms', parameters('appServicePlan_Galaxy.Linux_name'))]",
        "kind": "app",
        "httpsOnly": true
      },
      "resources": [
        {
          "name": "appsettings",
          "type": "config",
          "apiVersion": "2015-08-01",
          "dependsOn": [
            "[concat('Microsoft.Web/Sites/', parameters('appService_ACME-Sample_name'))]"
          ],
          "properties": {
            "WEBSITE_WEBDEPLOY_USE_SCM": "false"
          }
        }
      ]
    }
  ]
}